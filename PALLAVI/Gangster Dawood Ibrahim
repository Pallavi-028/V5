#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
typedef long long ll;

int main()
{
    int t ;
    cin>> t;
    while(t--)
    {
        
        ll n;
        cin>>n;
        vector<ll> a(n);
        for(ll i=0;i<n;i++) cin>>a[i];
        sort(a.begin(),a.end());
        cout<<a[(n-1)/2]<<"\n"; 
    }
    
    
    return 0;
}
